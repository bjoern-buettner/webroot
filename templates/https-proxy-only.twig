{% for host in virtualhosts %}
    <VirtualHost *:443>
        {% set domain=host.domain %}
        {% include 'https-proxy.twig' %}
    </VirtualHost>
    {% for domain in host.aliases %}
        <VirtualHost *:443>
            {% include 'https-proxy.twig' %}
        </VirtualHost>
    {% endfor %}
{% endfor %}